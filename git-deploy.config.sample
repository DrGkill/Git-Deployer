[engine-conf]
	git 		= /usr/bin/git
	mysql 		= /usr/bin/mysql
	error_file	= /tmp/git-deploy.err
	smtp		= smtp.example.com

[git-deploy]
        branch = master
        depth = 1
        user = git
        server = github.com
        git_project = git-deploy/git-deploy.git

        local_project_path = /home/test
	contact	= deploywatcher@example.com

        db_host = mydatabase
        db_port = 3306
        db_name = gitdeploy
        db_user = gitdeploy
        db_pass = gitdeploy_secret

[end]
